{
  "name": "my-blog",
  "version": "1.0.0",
  "description": "Host Blog.",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "dev": "node \"src/app/servers/\"",
    "preserve": "npm run build-public && npm run copy-static && npm run copy-funcs && npm run copy-funcs-index && npm run build-funcs && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "firebase serve",
    "predeploy": "npm run build-public && npm run copy-static && npm run copy-funcs && npm run copy-funcs-index && npm run build-funcs && npm run build-app && npm run copy-deps",
    "deploy": "cross-env NODE_ENV=production firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build-app": "next build \"src/app/\"",
    "copy-funcs": "cpx \"src/app/servers/**/!(index.js)\" \"src/functions\"",
    "copy-funcs-index": "cpx \"src/app/servers/api/index.js\" \"src/functions/api/\"",
    "copy-static": "cpx \"src/app/static/**/*.*\" \"dist/public/static\"",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i"
  },
  "dependencies": {
    "@editorjs/checklist": "^1.1.0",
    "@editorjs/code": "^2.4.1",
    "@editorjs/delimiter": "^1.1.0",
    "@editorjs/editorjs": "^2.15.1",
    "@editorjs/embed": "^2.2.1",
    "@editorjs/header": "^2.3.0",
    "@editorjs/image": "^2.3.1",
    "@editorjs/inline-code": "^1.3.0",
    "@editorjs/link": "^2.1.3",
    "@editorjs/list": "^1.4.0",
    "@editorjs/marker": "^1.2.1",
    "@editorjs/quote": "^2.3.0",
    "@editorjs/raw": "^2.1.1",
    "@editorjs/simple-image": "^1.3.2",
    "@editorjs/table": "^1.2.0",
    "@editorjs/warning": "^1.1.1",
    "@google-cloud/connect-firestore": "^1.0.3",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "connect-session-firebase": "^5.13.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "express-session": "^1.16.2",
    "firebase": "^6.3.0",
    "firebase-admin": "^8",
    "firebase-functions": "^3",
    "firebaseui": "^4.1.0",
    "firebaseui-ja": "^1.0.0",
    "image-to-base64": "^2.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "metascraper": "^5.6.5",
    "metascraper-date": "^5.6.5",
    "metascraper-description": "^5.6.5",
    "metascraper-image": "^5.6.5",
    "metascraper-logo": "^5.6.5",
    "metascraper-logo-favicon": "^5.6.5",
    "metascraper-title": "^5.6.5",
    "metascraper-url": "^5.6.5",
    "metascraper-video": "^5.6.5",
    "metascraper-youtube": "^5.6.5",
    "next": "9.0.5",
    "nprogress": "^0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-infinite-scroll-component": "^4.5.3",
    "react-select": "^3.0.4",
    "sanitize-html": "^1.20.1",
    "session-file-store": "^1.3.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^7.3.2",
    "rimraf": "^2.6.0"
  }
}
